{
  "name": "lanyon",
  "version": "0.0.43",
  "description": "dr. Jekyll has turned into mr. Hyde. Lanyon to the rescue!",
  "main": "config.js",
  "bin": {
    "lanyon": "cli.js"
  },
  "scripts": {
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "install": "node cli.js postinstall",
    "container:build": "env DOCKER_BUILD=1 DEBUG=*:* LANYON_ONLY=docker LANYON_PROJECT=$(pwd)/example node cli.js postinstall",
    "release:major": "env SEMANTIC=major npm run release",
    "release:minor": "env SEMANTIC=minor npm run release",
    "release:patch": "env SEMANTIC=patch npm run release",
    "release": "npm version ${SEMANTIC:-patch} -m \"Release %s\" && npm run container:build && git push && git push --tags && npm publish",
    "test": "./test.sh"
  },
  "lanyon": {
    "statistics": "stats.html",
    "entries": [
      "app"
    ],
    "prerequisites": {
      "sh": {
        "preferred": "0.5.7",
        "range": ">=0.0.1",
        "writeShim": true
      },
      "node": {
        "preferred": "4.6.1",
        "range": ">=0.10"
      },
      "docker": {
        "preferred": "1.12.3",
        "range": ">=1.12"
      },
      "rbenv": {
        "preferred": "1.0.0",
        "range": ">=1"
      },
      "rvm": {
        "preferred": "1.27.0",
        "range": ">=1"
      },
      "ruby": {
        "preferred": "2.3.3",
        "range": ">=2",
        "writeShim": true
      },
      "gem": {
        "preferred": "2.0.14.1",
        "range": ">=2",
        "writeShim": true
      },
      "bundler": {
        "preferred": "1.13.0",
        "range": ">=1",
        "writeShim": true
      },
      "jekyll": {
        "preferred": "3.3.0",
        "range": ">=3",
        "writeShim": true
      }
    },
    "ports": {
      "assets": 3000,
      "content": 4000
    },
    "gems": {
      "github-pages": "104",
      "jekyll-crosspost-to-medium": "0.1.14",
      "stringex": "2.6.0"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kvz/lanyon.git"
  },
  "keywords": [
    "jekyll",
    "static",
    "site",
    "generator"
  ],
  "author": "Kevin van Zonneveld",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kvz/lanyon/issues"
  },
  "homepage": "https://github.com/kvz/lanyon#readme",
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "node": true
    }
  },
  "devDependencies": {
    "eslint": "3.10.2",
    "eslint-config-standard": "6.2.1",
    "eslint-plugin-promise": "3.4.0",
    "eslint-plugin-standard": "2.0.1"
  },
  "dependencies": {
    "assets-webpack-plugin": "^3.5.0",
    "autoprefixer": "6.5.3",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.8",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "bower-webpack-plugin": "^0.1.9",
    "browser-sync": "2.18.2",
    "chalk": "1.1.3",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.11.1",
    "css-loader": "0.26.0",
    "depurar": "0.3.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "1.0.1",
    "fakefile": "0.0.8",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "font-awesome-loader": "1.0.1",
    "globby": "^6.1.0",
    "http-server": "0.9.0",
    "imports-loader": "^0.6.5",
    "js-yaml": "^3.7.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "lodash": "4.17.2",
    "node-libs-browser": "2.0.0",
    "node-sass": "3.13.0",
    "nodemon": "1.11.0",
    "npm-run-all": "3.1.1",
    "parallelshell": "^2.0.0",
    "postcss-loader": "1.1.1",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-hot-loader": "3.0.0-beta.6",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "sass-loader": "4.0.2",
    "semver": "5.3.0",
    "shelljs": "0.7.5",
    "spawn-sync": "^1.0.15",
    "style-loader": "0.13.1",
    "uglify": "^0.1.5",
    "uglify-loader": "1.3.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "1.16.2",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-md5-hash": "^0.0.5",
    "webpack-visualizer-plugin": "^0.1.5"
  }
}
